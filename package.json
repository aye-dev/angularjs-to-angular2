    "license": "MIT",
    "main": "index.js",
    "dependencies": {
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "@angular/upgrade": "8.2.14",
        "angular": "1.8.0",
        "angular-route": "1.5.0",
        "core-js": "2.5.4",
        "rxjs": "6.5.4",
        "zone.js": "0.10.2"
    },
    "devDependencies": {
        "@angular/compiler-cli": "8.2.14",
        "@ngtools/webpack": "8.3.22",
        "@types/angular": "1.6.57",
        "@types/core-js": "2.5.2",
        "@types/node": "8.0.0",
        "ts-loader": "6.2.1",
        "angular2-template-loader": "0.6.2",
        "cpx": "1.5.0",
        "css-loader": "3.4.1",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "null-loader": "3.0.0",
        "raw-loader": "4.0.0",
        "rimraf": "2.5.4",
        "style-loader": "1.1.2",
        "typescript": "3.7.4",
        "webpack": "4.41.5",
        "webpack-bundle-analyzer": "3.6.0",
        "webpack-cli": "3.3.10"
    },
    "scripts": {
        "build": "npm run clean-dev && npm run copy-libs && webpack --config config/webpack.dev.js --watch --profile --progress",
        "start": "http-server -a localhost -p 8000 -c-1 dist/dev",
        "copy-libs": "cpx \"node_modules/{angular,angular-*,html5-boilerplate/dist}/**/*\" dist/dev/lib -C",
        "clean-dev": "rimraf dist/dev/*"
    }
}
